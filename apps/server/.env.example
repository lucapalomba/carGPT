# Environment variables
APP_ENV=development
PORT=3000
SESSION_SECRET=your_session_secret_here

# The Ollama model to use for vision tasks (e.g., llava, bakllava).
# Dedicated vision models are recommended for better image verification.
OLLAMA_VISION_MODEL=llava

# Vision Verification Thresholds
# Minimum confidence (0.0 - 1.0) required for the AI to confirm the car model matches.
VISION_MODEL_CONFIDENCE_THRESHOLD=0.8

# Maximum allowed confidence (0.0 - 1.0) for overlay text/watermarks.
# Images with text confidence HIGHER than this value will be rejected.
VISION_TEXT_CONFIDENCE_THRESHOLD=0.2

# Google Custom Search API (for car images)
# Get from: https://programmablesearchengine.google.com/
GOOGLE_API_KEY=your_google_api_key_here

# Number of images to display in the carousel for each car (max 10)
CAROUSEL_IMAGES_LENGHT=10

# Langfuse
LANGFUSE_PUBLIC_KEY=your-pk
LANGFUSE_SECRET_KEY=your-sk
